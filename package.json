{
    "name": "resource-peek",
    "displayName": "Resource Peek",
    "description": "Peek for the text values of ngx-translate translation keys.",
    "version": "0.1.1",
    "license": "MIT",
    "publisher": "Hademar",
    "engines": {
        "vscode": "^1.10.0"
    },
    "icon": "images/logo.svg",
    "categories": [
        "Languages",
        "Other"
    ],
    "keywords": [
        "angular",
        "ngx-translate",
        "resource",
        "translation",
        "peek"
    ],
    "activationEvents": [
        "onLanguage:html"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "@types/node": "^6.0.40"
    }
}